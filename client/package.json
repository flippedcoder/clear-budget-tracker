    },
    "verbose": true,
    "pathToJest": "client/node_modules/.bin/jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
